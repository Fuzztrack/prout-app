+// Ensure react-native-worklets is built before reanimated CMake targets (all ABIs)
+project.afterEvaluate {
+    // Reuse workletsProject variable from top-level scope
+    if (workletsProject) {
+        // First, ensure all worklets externalNativeBuild tasks complete before any reanimated CMake tasks
+        def workletsExternalBuildTask = workletsProject.tasks.findByName("externalNativeBuildRelease")
+        if (!workletsExternalBuildTask) {
+            workletsExternalBuildTask = workletsProject.tasks.findByName("externalNativeBuild")
+        }
+        
+        // Robust wiring: use configureEach to attach deps even if tasks are created later
+        // 1) All configureCMake/buildCMake (reanimated) must run after worklets externalNativeBuild
+        workletsProject.tasks.matching { it.name in ["externalNativeBuildRelease", "externalNativeBuild"] }
+            .configureEach { wTask ->
+                tasks.matching { it.name =~ /(configureCMake|buildCMake).*\\[.*\\]\\[reanimated\\]/ }
+                    .configureEach { rTask ->
+                        rTask.dependsOn(wTask)
+                        rTask.mustRunAfter(wTask)
+                    }
+            }
+
+        // 2) CRITICAL: configureCMake[abi][reanimated] must wait for buildCMake[abi][worklets] for the same ABI
+        // This ensures worklets is built for a specific ABI before reanimated configures for that same ABI
+        // This is critical because Gradle can parallelize ABIs, so we need ABI-specific dependencies
+        tasks.matching { it.name =~ /configureCMake.*\\[([^\\]]+)\\]\\[reanimated\\]/ }
+            .configureEach { rTask ->
+                def m = (rTask.name =~ /configureCMake.*\\[([^\\]]+)\\]\\[reanimated\\]/)
+                if (m.matches()) {
+                    def abi = m[0][1]
+                    // Try to find the corresponding buildCMake task for worklets with the same ABI
+                    def workletsBuildTaskName = "buildCMakeRelWithDebInfo[${abi}][worklets]"
+                    def workletsBuildTask = workletsProject.tasks.findByName(workletsBuildTaskName)
+                    if (workletsBuildTask) {
+                        rTask.dependsOn(workletsBuildTask)
+                        rTask.mustRunAfter(workletsBuildTask)
+                    } else {
+                        // Fallback: wait for externalNativeBuildRelease if specific task not found
+                        def workletsExternalBuildRelease = workletsProject.tasks.findByName("externalNativeBuildRelease")
+                        if (workletsExternalBuildRelease) {
+                            rTask.dependsOn(workletsExternalBuildRelease)
+                            rTask.mustRunAfter(workletsExternalBuildRelease)
+                        }
+                    }
+                }
+            }
+        
+        // Also use whenTaskAdded to catch tasks created later
+        tasks.whenTaskAdded { rTask ->
+            if (rTask.name =~ /configureCMake.*\\[([^\\]]+)\\]\\[reanimated\\]/) {
+                def m = (rTask.name =~ /configureCMake.*\\[([^\\]]+)\\]\\[reanimated\\]/)
+                if (m.matches()) {
+                    def abi = m[0][1]
+                    def workletsBuildTaskName = "buildCMakeRelWithDebInfo[${abi}][worklets]"
+                    def workletsBuildTask = workletsProject.tasks.findByName(workletsBuildTaskName)
+                    if (workletsBuildTask) {
+                        rTask.dependsOn(workletsBuildTask)
+                        rTask.mustRunAfter(workletsBuildTask)
+                    } else {
+                        def workletsExternalBuildRelease = workletsProject.tasks.findByName("externalNativeBuildRelease")
+                        if (workletsExternalBuildRelease) {
+                            rTask.dependsOn(workletsExternalBuildRelease)
+                            rTask.mustRunAfter(workletsExternalBuildRelease)
+                        }
+                    }
+                }
+            }
+        }
+
+        // 3) Granular: buildCMake[abi][reanimated] waits for buildCMake[abi][worklets] if present
+        tasks.matching { it.name =~ /buildCMake.*\\[.*\\]\\[reanimated\\]/ }
+            .configureEach { rTask ->
+                def m = (rTask.name =~ /buildCMake.*\\[([^\\]]+)\\]\\[reanimated\\]/)
+                if (m.matches()) {
+                    def abi = m[0][1]
+                    def workletsBuildTaskName = rTask.name.replace("reanimated", "worklets")
+                    def workletsBuildTask = workletsProject.tasks.findByName(workletsBuildTaskName)
+                    if (workletsBuildTask) {
+                        rTask.dependsOn(workletsBuildTask)
